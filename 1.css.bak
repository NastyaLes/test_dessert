* {
	margin: 0px;
	padding: 0px;
}
header {
    background: #F0E68C;
    border: 4px solid #BDB76B;
	margin-left: 30px;
	margin-right: 30px;
	margin-top: 10px;
	margin-bottom: 10px;
	padding-top: 0px;
	padding-bottom: 0px;
	display: block;
}
nav {
    background: #FFE4B5;
    border: 4px solid #FF7F50;
	margin-left: 30px;
	margin-right: 70%;
	padding-left: 60px;
	padding-top: 10px;
	padding-bottom: 10px;
	display: block;
}
figure {
    background: #FFD700;
    border: 4px solid #FF7F50;
	margin-left: 30%;
	margin-right: 30px;
	margin-bottom: 10px;
	display: inline-block;
}
footer {
    background: #F0E68C;
    border: 4px solid #BDB76B;
	margin-left: 30px;
	margin-right: 30px;
	margin-top: 10px;
	margin-bottom: 10px;
	padding-top: 0px;
	padding-bottom: 0px;
	display: block;
}